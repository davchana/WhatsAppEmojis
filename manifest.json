{
    "name": "WhatsApp Web Emoji Converter",
    "description": "Convert ASCII smileys to WhatsApp emojis in WhatsApp Web",
    "version": "0.2",
    "permissions": [
        "https://web.whatsapp.com/"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "manifest_version":2
}
